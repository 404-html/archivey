[
    {
        "client_msg_id": "d094556d-5cfc-44c1-b811-754ae0537549",
        "type": "message",
        "text": "<@U15J5H60P> is app background processing possible with ios?",
        "user": "U15HRTAHL",
        "ts": "1534445170.000100"
    },
    {
        "client_msg_id": "65dfafa0-f38f-486c-8785-df143a088977",
        "type": "message",
        "text": "Like when the app isn’t even open it can still process stuff periodically in the background?",
        "user": "U15HRTAHL",
        "ts": "1534445191.000100"
    },
    {
        "client_msg_id": "f686b005-1b3a-4097-a5b4-92d571093beb",
        "type": "message",
        "text": "Yes - but there are limitations",
