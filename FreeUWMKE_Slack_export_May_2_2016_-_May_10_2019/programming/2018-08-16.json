    {
        "client_msg_id": "61ff7bee-58ca-4845-a71f-d1f5530e89d6",
        "type": "message",
        "text": "Cool",
        "user": "U15HRTAHL",
        "ts": "1534445306.000100"
    },
    {
        "client_msg_id": "34d85ea0-7848-488b-aadd-3e8d0a5a0682",
        "type": "message",
        "text": "Not sure if you can post it to a server.",
