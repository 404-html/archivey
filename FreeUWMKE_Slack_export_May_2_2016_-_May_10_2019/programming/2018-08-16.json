    },
    {
        "client_msg_id": "4b8c5887-6aaf-435a-bc02-babe2007e928",
        "type": "message",
        "text": "You can communicate with external bluetooth devices",
        "user": "U15J5H60P",
        "ts": "1534445293.000100"
    },
    {
        "client_msg_id": "2ad8069c-6691-43f1-9455-4d8f01869f49",
        "type": "message",
        "text": "Update location",
        "user": "U15J5H60P",
        "ts": "1534445296.000100"
    },
    {
        "client_msg_id": "593a76c6-188f-4cca-aa22-ecff44870c70",
        "type": "message",
        "text": "What about getting current location and posting that to a server?",
        "user": "U15HRTAHL",
        "ts": "1534445302.000100"
